-- 1
SELECT f_first, f_last
FROM faculty
WHERE f_rank='FULL' OR f_rank = 'INST';

-- 2
SELECT f_first, f_last
FROM faculty
WHERE f_rank LIKE 'FULL' OR f_rank LIKE 'INST';

-- 3
SELECT DISTINCT s_class
FROM student;

-- 4
SELECT s_first, s_last, s_dob
FROM student
WHERE s_dob BETWEEN '1985-01-01' AND '1985-12-31';

-- 5
SELECT s_first, s_last, s_dob
FROM student
WHERE s_dob >= '1985-01-01' AND s_dob <= '1985-12-31';

-- 6
SELECT s_first, s_last, s_mi
FROM student
WHERE s_mi IS NOT NULL;

--7
SELECT s_id
FROM enrollment
WHERE grade IS NULL;

--8
SELECT DISTINCT f_id
FROM student; 

--9
SELECT f_id
FROM student
GROUP BY f_id;

--10
SELECT DISTINCT loc_id
FROM faculty;

--11
SELECT DISTINCT loc_id
FROM course_section 

--12
SELECT c_sec_id
FROM course_section
WHERE c_sec_day NOT LIKE '%W%';

--13
SELECT c_sec_id
FROM course_section
WHERE c_sec_day NOT LIKE '%W%' OR c_sec_day NOT LIKE '%F%';

--14
SELECT course_id, term_id, AVG(max_enrl)
FROM course_section
WHERE course_id = '1' AND term_id = '4';

--15
SELECT COUNT(grade)
FROM enrollment
WHERE grade = 'B' AND grade IS NOT NULL;

--16.1
SELECT f_id, COUNT(*) AS student_count
FROM student
GROUP BY f_id;

--16.2
SELECT f.f_id, COUNT(s.s_id) AS num_students
FROM faculty AS f JOIN student AS s ON (f.f_id = s.f_id)
GROUP BY f.f_id;

--17
SELECT *
FROM enrollment
WHERE grade = 'C' OR grade IS NULL;

--18
SELECT loc_id, COUNT(c_sec_id)
FROM course_section
GROUP BY loc_id
ORDER BY COUNT(c_sec_id) DESC;

--19
SELECT loc_id, COUNT(c_sec_id)
FROM course_section
GROUP BY loc_id
HAVING COUNT(c_sec_id) > 3
ORDER BY COUNT(c_sec_id) DESC;